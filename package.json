{
  "name": "running",
  "version": "1.0.0",
  "private": true,
  "engineStrict": true,
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "data:clean": "rm scripts/data.db GPX_OUT/* activities/* src/static/activities.json",
    "data:download:garmin": "python3 scripts/garmin_sync.py",
    "data:analysis": "python3 scripts/gen_svg.py --from-db --type github --output assets/github.svg",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "serve": "gatsby serve",
    "lint": "eslint --ext .js,.jsx src --fix",
    "check": "npx prettier --write *.{js,jsx,scss,json,md,yaml}",
    "ci": "yarn run check && yarn run build && yarn run test"
  },
  "dependencies": {
    "@mapbox/mapbox-gl-language": "1.0.0",
    "@mapbox/polyline": "1.1.1",
    "@math.gl/web-mercator": "^3.5.7",
    "@visx/axis": "2.6.0",
    "@visx/group": "2.1.0",
    "@visx/legend": "2.2.2",
    "@visx/responsive": "2.8.0",
    "@visx/scale": "2.2.2",
    "@visx/shape": "2.4.0",
    "gatsby": "4.12.1",
    "gatsby-alias-imports": "1.0.6",
    "gatsby-plugin-react-helmet": "5.12.1",
    "gatsby-plugin-react-svg": "3.1.0",
    "gatsby-plugin-robots-txt": "1.7.1",
    "gatsby-plugin-sass": "5.12.1",
    "gatsby-plugin-sharp": "4.12.1",
    "gatsby-plugin-sitemap": "5.12.1",
    "gatsby-remark-autolink-headers": "5.12.1",
    "gatsby-remark-classes": "1.0.2",
    "gatsby-remark-images": "6.12.1",
    "gatsby-remark-responsive-iframe": "5.12.1",
    "gatsby-remark-smartypants": "5.12.1",
    "gatsby-remark-widows": "1.0.0",
    "gatsby-source-filesystem": "4.12.1",
    "gatsby-transformer-json": "4.12.1",
    "gatsby-transformer-remark": "5.12.1",
    "gatsby-transformer-sharp": "4.12.1",
    "gcoord": "0.3.2",
    "mapbox-gl": "^2.8.1",
    "prop-types": "15.8.1",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-helmet": "6.1.0",
    "react-map-gl": "5.3.19",
    "sass": "1.50.1",
    "sass-mq": "6.0.0",
    "styled-components": "5.3.5",
    "tachyons": "4.12.0",
    "tachyons-sass": "4.9.5",
    "webpack": "5.72.0"
  },
  "devDependencies": {
    "babel-eslint": "10.1.0",
    "eslint": "8.14.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.29.4",
    "eslint-plugin-react-hooks": "4.4.0",
    "prettier": "2.6.2"
  },
  "packageManager": "yarn@3.2.0"
}
